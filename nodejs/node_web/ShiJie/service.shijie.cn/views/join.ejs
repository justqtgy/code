<%- include ./shared/_header %>
<!-- BEGIN PAGE LEVEL PLUGINS -->
<link href="../assets/global/plugins/bootstrap-datepicker/datepicker3.css" rel="stylesheet" type="text/css" />
<link href="../stylesheets/pricing.css" rel="stylesheet" type="text/css" />
<!-- END PAGE LEVEL PLUGINS -->
<%- include ./shared/_top %>
<%- include ./shared/_sidebar %>

	<!-- BEGIN CONTENT -->
	<div class="page-content-wrapper">
	 <div class="page-content">
		<!-- BEGIN PAGE HEADER-->
		<h1 class="page-title">
		我要代理 <small>全国统一零售价158元/盒</small>
		</h1>
		<!-- END PAGE HEADER-->
		
		<!-- BEGIN PAGE CONTENT-->
		<div class="portlet light portlet-fit bordered">
			<div class="portlet-title">
				<div class="caption">
					<i class="icon-share font-green"></i>
					<span class="caption-subject font-green bold uppercase">我要代理</span>
				</div>
				 
			</div>
			<div class="portlet-body">
				<div class="pricing-content-1">
					<div class="row" id="grid">
						<div class="col-md-3">
							<div class="price-column-container border-active">
								<div class="price-table-head bg-green">
									<h2 class="no-margin">{{ thPricing.title }}</h2>
								</div>
								<div class="arrow-down border-top-green"></div>
								<div class="price-table-pricing">
									<h3>
										<sup class="price-sign">¥</sup>{{thPricing.amount}}</h3>
									<p>&nbsp</p>
								</div>
								<div class="price-table-content">
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-feed"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">单价：¥{{thPricing.price}}</div>
									</div>
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-drawer"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">首批提货量：{{thPricing.number}}(支)</div>
									</div>
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-screen-smartphone"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">金额：¥{{thPricing.amount}}</div>
									</div>
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-refresh"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">补货量：{{thPricing.number}}(支)</div>
									</div>
								</div>
								<div class="arrow-down arrow-grey"></div>
								<div class="price-table-footer">
									<button type="button" class="btn green price-button sbold uppercase" @click="join('tanhua')">快速下单</button>
								</div>
							</div>
						</div>
						<div class="col-md-3">
							<div class="price-column-container border-active">
								<div class="price-table-head bg-purple">
									<h2 class="no-margin">{{zyPricing.title}}</h2>
								</div>
								<div class="arrow-down border-top-purple"></div>
								<div class="price-table-pricing">
									<h3>
										<sup class="price-sign">¥</sup>{{zyPricing.amount}}</h3>
									<p>&nbsp</p>
									<div class="price-ribbon">热销</div>
								</div>
								<div class="price-table-content">
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-feed"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">单价：¥{{zyPricing.price}}</div>
									</div>
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-drawer"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">首批提货量：{{zyPricing.first}}(支)</div>
									</div>
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-screen-smartphone"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">金额：¥{{zyPricing.amount}}</div>
									</div>
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-refresh"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">补货量：{{zyPricing.number}}(支)</div>
									</div>
								</div>
								<div class="arrow-down arrow-grey"></div>
								<div class="price-table-footer">
									<button type="button" class="btn green price-button sbold uppercase" @click="join('zhuangyuan')">快速下单</button>
								</div>
							</div>
						</div>
						<div class="col-md-3">
							<div class="price-column-container border-active">
								<div class="price-table-head bg-red">
									<h2 class="no-margin">{{byPricing.title}}</h2>
								</div>
								<div class="arrow-down border-top-red"></div>
								<div class="price-table-pricing">
									<h3>
										<sup class="price-sign">¥</sup>{{byPricing.amount}}</h3>
									<p>&nbsp</p>
									
								</div>
								<div class="price-table-content">
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-feed"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">单价：¥{{byPricing.price}}</div>
									</div>
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-drawer"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">首批提货量：{{byPricing.first}}(支)</div>
									</div>
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-screen-smartphone"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">金额：¥{{byPricing.amount}}</div>
									</div>
									<div class="row mobile-padding">
										<div class="col-xs-3 text-right mobile-padding">
											<i class="icon-refresh"></i>
										</div>
										<div class="col-xs-9 text-left mobile-padding">补货量：{{byPricing.number}}(支)</div>
									</div>
								</div>
								<div class="arrow-down arrow-grey"></div>
								<div class="price-table-footer">
									<button type="button" class="btn green price-button sbold uppercase" @click="join('bangyan')">快速下单</button>
								</div>
							</div>
						</div>
						
					</div>
				</div>
			</div>
		</div>
		<!-- END PAGE CONTENT-->
		 
	  </div>
	</div>
	<!-- END CONTENT -->
</div>
<!-- END CONTAINER -->


