<%- include ./shared/_header %>
    <!-- BEGIN PAGE LEVEL PLUGINS -->

    <!-- END PAGE LEVEL PLUGINS -->
<%- include ./shared/_top %>
<%- include ./shared/_sidebar %>

    <!-- BEGIN CONTENT -->
    <div class="page-content-wrapper">
	     <div class="page-content">
		    <!-- BEGIN PAGE HEADER-->
		    <h3 class="page-title">
		    实时数据 <small>......</small>
		    </h3>
		    <!-- END PAGE HEADER-->
		    
			<!-- BEGIN PAGE CONTENT-->
            <div class="row" id="grid">
				<% result.forEach(function(item){ %>
					<div class="col-md-4 col-sm-4">
						<div class="portlet light ">
							<div class="portlet-title">
								<div class="caption">
									<i class="icon-cursor font-dark hide"></i>
									<span class="caption-subject font-dark bold uppercase"><%= item.gps_name + '('+item.gps_id+')' %></span>
								</div>                                    
							</div>
							<div class="portlet-body">
								<div class="row">
									<div class="col-md-4">
										<div class="easy-pie-chart">
											<div class="number rl" data-percent="<%= item.one_rl %>">
												<span><%= item.one_rl %></span>% </div>
											<a class="title" href="javascript:;"> 容量1
												<i class="icon-arrow-right"></i>
											</a>
										</div>
									</div>
									<% if (item.type !=1) {%>
									<div class="col-md-4" >
										<div class="easy-pie-chart">
											<div class="number rl" data-percent=" <%= item.two_rl %> ">
												<span><%= item.two_rl %></span>% </div>
											<a class="title" href="javascript:;"> 容量2
												<i class="icon-arrow-right"></i>
											</a>
										</div>
									</div>
									<% }%>
									<div class="margin-bottom-10 visible-sm"> </div>
									<div class="col-md-4">
										<div class="easy-pie-chart">
											<div class="number dl" data-percent="<%= item.power %>">
												<span><%= item.power %></span>% </div>
											<a class="title" href="javascript:;"> 电量
												<i class="icon-arrow-right"></i>
											</a>
										</div>
									</div>
									
								</div>
							</div>
						</div>
					</div>
				<% }) %>
			</div>
                    
			<!-- END PAGE CONTENT-->
		</div>
	</div>
	<!-- END CONTENT -->

<%- include shared/_bottom %>
<%- include shared/_scripts %>
	<!-- BEGIN PAGE LEVEL PLUGINS -->
	<script src="../assets/global/plugins/jquery-easypiechart/jquery.easypiechart.min.js" type="text/javascript"></script>
	<!-- END PAGE LEVEL PLUGINS -->
	<script src="/javascripts/include/query.js"></script>
	<script src="/javascripts/last.js" type="text/javascript"></script>
<%- include shared/_footer %>
