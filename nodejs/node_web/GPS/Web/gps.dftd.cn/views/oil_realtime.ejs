<%- include shared/_top %>
<%- include shared/_sidebar %>
        <div id="loading">
			<img src="/images/loading.gif" alt="loading" />
		</div>
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
		      <div class="page-content">
            <!-- BEGIN PAGE HEADER-->
            <h3 class="page-title">
            实时油量查询 <small>查询实时油量</small>
            </h3>
             
            <div class="box box-solid">
                <div class="box-header with-border">
                    <i class="fa fa-search"></i>
                    <h3 class="box-title">查询</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
					<form  id='search'>        
						
						<label class="control-label" for="selCar">车辆选择：</label><select class="multiselect display-none" multiple="multiple" ></select>
						<button type="button" id="btnSearch" class="btn btn-success"> <i class="fa fa-search"></i>查询 </button>
					</form>
                </div><!-- /.box-body -->
            </div>
            
            <!-- END PAGE HEADER-->
            
            <!-- BEGIN PAGE CONTENT-->
            <div class="row">
              <div class="col-md-12">
                <div class="portlet box green">
					<div class="portlet-title">
						<div class="caption">
							<i class="fa fa-picture"></i> 数据列表
						</div>
						
					</div>
					<div class="portlet-body">
						<div class="table-scrollable">
							<table id="grid" class="table table-striped table-bordered table-hover dataTable">
							<thead>
							<tr>           
                                <th>ID</th>    
								<th>车牌号</th> 
								<th>设备号</th> 
								<th>油量状态</th>
								<th>当前油量</th>       
								<th>时间点</th>                               
							</tr>
							</thead>
							<tbody>
							
							</tbody>
							</table>						
						</div>						 
					</div>
				</div>
              </div>
            </div>
            <!-- END PAGE CONTENT-->

          </div>
        </div>
        <!-- END CONTENT -->
    </div>
