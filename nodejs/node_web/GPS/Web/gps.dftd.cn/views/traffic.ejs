<%- include shared/_header %>
<link href="/plugins/bootstrap-datetimepicker/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
<%- include shared/_top %>
<%- include shared/_sidebar %>
        <div id="loading">
			<img src="/images/loading.gif" alt="loading" />
		</div>
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
		      <div class="page-content">
            <!-- BEGIN PAGE HEADER-->
            <h3 class="page-title">
            实时查询 <small>查询实时油耗距离</small>
            </h3>
             
            <div class="box box-solid">
                <div class="box-header with-border">
                    <i class="fa fa-search"></i>
                    <h3 class="box-title">查询</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
					<form  id='search'>         
						<label class="control-label" for="txtBeginTime">开始时间：</label><input type="text" id="txtBeginTime" name="begintime" class="form_datetime"  value="<%= start_date %>"/>
						<label class="control-label" for="txtEndTime">结束时间：</label><input type="text" id="txtEndTime" name="endtime" class="form_datetime" value="<%= end_date %>"/>						
						<label class="control-label" for="selCar">车辆选择：</label><select class="multiselect display-none" multiple="multiple" ></select>
						<button type="button" id="btnSearch" class="btn btn-success"> <i class="fa fa-search"></i>查询 </button>
					</form>
                </div><!-- /.box-body -->
            </div>
            
            <!-- END PAGE HEADER-->
            
            <!-- BEGIN PAGE CONTENT-->
            <div class="row">
              <div class="col-md-12">
                <div class="portlet box green">
					<div class="portlet-title">
						<div class="caption">
							<i class="fa fa-picture"></i> 数据列表
						</div>
						
					</div>
					<div class="portlet-body">
						<div class="table-scrollable">
							<table id="grid" class="table table-striped table-bordered table-hover dataTable">
							<thead>
							<tr>               
								<th>车牌号</th> 
								<th>开始时间</th> 
								<th>结束时间</th>
								<th>距离</th>       
								<th>油耗</th>                               
							</tr>
							</thead>
							<tbody>
							
							</tbody>
							</table>						
						</div>						 
					</div>
				</div>
              </div>
            </div>
            <!-- END PAGE CONTENT-->

          </div>
        </div>
        <!-- END CONTENT -->
    </div>
    <!-- END CONTAINER -->
    
<%- include shared/_bottom %>
<%- include shared/_scripts %>
	<script src="/plugins/bootbox/bootbox.min.js" type="text/javascript"></script>
	<script src="/plugins/pagination/bootstrap-paginator.js"></script>
	<script src="/javascripts/include/date-utils.min.js"></script>
	<script src="/javascripts/traffic.js" type="text/javascript"></script>
	<script src="/plugins/bootstrap-datetimepicker/js/bootstrap-datetimepicker.min.js"></script>
	<script src="/plugins/bootstrap-datetimepicker/js/locales/bootstrap-datetimepicker.zh-CN.js"></script>
<%- include ./control/_group %> 
    <script>
        $(function(){
			$("#txtBeginTime, #txtEndTime").datetimepicker({ format: 'yyyy-mm-dd hh:ii:ss' });
			$("#btnSearch").click(function(){
					get_list(1);
				});		
		});
    </script>
<%- include shared/_footer %>
