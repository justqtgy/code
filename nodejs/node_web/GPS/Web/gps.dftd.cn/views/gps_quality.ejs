<%- include shared/_header %>
<%- include shared/_top %>
<%- include shared/_sidebar %>
        <div id="loading">
			<img src="/images/loading.gif" alt="loading" />
		</div>

        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
		      <div class="page-content">
            <!-- BEGIN PAGE HEADER-->
            <h3 class="page-title">
            油品分析 <small>说明</small>
            </h3>
            
            <!-- END PAGE HEADER-->
            <div class="box box-solid">
                <div class="box-header with-border">
                    <i class="fa fa-search"></i>
                    <h3 class="box-title">查询</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                    <label class="control-label" for="txtBeginTime">开始时间：</label><input type="text" id="txtBeginTime" class="form_datetime"  value="<%= start_date %>"/>
										<label class="control-label" for="txtEndTime">结束时间：</label><input type="text" id="txtEndTime" class="form_datetime" value="<%= end_date %>"/>

                    <button type="button" id="btnSearch" class="btn btn-success"> <i class="fa fa-search"></i>查询 </button>
                </div><!-- /.box-body -->
            </div>
            <!-- BEGIN PAGE CONTENT-->
            <div class="row">
              <div class="col-md-12">
                <div class="portlet box green">
					<div class="portlet-title">
						<div class="caption">
							<i class="fa fa-picture"></i> 数据列表
						</div>
						 
					</div>
					<div class="portlet-body">
						<div class="table-scrollable">
							<table id="grid" class="table table-striped table-bordered table-hover dataTable">
							<thead>
							<tr>
											<th>ID</th> 
			<th>设备号</th> 
			<th>C1</th> 
			<th>C2</th> 
			<th>空瓶值</th> 
			<th>满瓶值</th> 
			<th>初始值</th> 
			<th>体积</th> 
			<th>油品</th> 
			<th>时间</th> 
			 

							</tr>
							</thead>
							<tbody>
							
							</tbody>
							</table>							
						</div>
						<div>
							<ul id="Pagination" class="pagination"></ul>
						</div>
					</div>
				</div>
              </div>
            </div>
            <!-- END PAGE CONTENT-->

            
          </div>
        </div>
        <!-- END CONTENT -->
    </div>
    <!-- END CONTAINER -->
    
<%- include shared/_bottom %>
<%- include shared/_scripts %>
	<script src="/plugins/bootbox/bootbox.min.js" type="text/javascript"></script>
	<script src="/plugins/pagination/bootstrap-paginator.js"></script>
	<script src="/javascripts/include/date-utils.min.js"></script>
	<script src="/javascripts/gps_quality.js" type="text/javascript"></script>
  <script src="/plugins/datepicker/bootstrap-datepicker.js"></script>
	<script src="/plugins/datepicker/locales/bootstrap-datepicker.zh-CN.js"></script>

    <script>
        $(function(){
					$("#txtBeginTime, #txtEndTime").datepicker({ format: 'yyyy-mm-dd' });
					$("#btnSearch").click(function(){
						get_list(1);
					});
					get_list(1);
				});
    </script>
<%- include shared/_footer %>
