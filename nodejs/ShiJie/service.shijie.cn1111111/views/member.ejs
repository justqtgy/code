<%- include ./shared/_header %>
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <link href="../assets/global/plugins/bootstrap-datepicker/datepicker3.css" rel="stylesheet" type="text/css" />
    <!-- END PAGE LEVEL PLUGINS -->
<%- include ./shared/_top %>
<%- include ./shared/_sidebar %>    
        
        <!-- BEGIN CONTENT -->
        <div class="page-content-wrapper">
	     <div class="page-content">
		    <!-- BEGIN PAGE HEADER-->
		    <h3 class="page-title">
		    Member <small>说明</small>
		    </h3>
		    <div class="page-bar">
		      <ul class="page-breadcrumb">
			<li>
			  <i class="fa fa-home"></i>
			  <a href="index.html">Home</a>
			  <i class="fa fa-angle-right"></i>
			</li>               
			<li>
			  <a href="#">Member</a>
			</li>
		      </ul>
		    </div>
		    <!-- END PAGE HEADER-->
		    <!-- END PAGE HEADER-->
		    <div class="box box-solid">
			<div class="box-header with-border">
			    <i class="fa fa-search"></i>
			    <h3 class="box-title">查询</h3>
			</div><!-- /.box-header -->
			<div class="box-body">
			    <form  id='search'>                 
				<label class="control-label" for="txtBeginTime">开始时间：</label>
				<input type="text" name="begintime"  class="date-picker input-control" value="<%= start_date %>"  data-date-format="yyyy-mm-dd"/>
				<label class="control-label" for="txtEndTime">结束时间：</label>
				<input type="text" name="endtime"  class="date-picker input-control" value="<%= end_date %>"  data-date-format="yyyy-mm-dd"/>
				<button type="button" id="btnSearch" class="btn btn-success"> <i class="fa fa-search"></i>查询 </button>
			    </form>
			</div><!-- /.box-body -->
		    </div>

            <!-- BEGIN PAGE CONTENT-->
            <div class="row">
              <div class="col-md-12">
                <div class="portlet box green">
			<div class="portlet-title">
				<div class="caption">
					<i class="fa fa-picture"></i> 数据列表
				</div>
				<div class="actions">
					<a href="javascript:;" class="btn btn-default btn-sm" onclick="show_modal();">
					<i class="fa fa-pencil"></i> 新增 </a>							
				</div>
			</div>
			<div class="portlet-body">
				<div class="table-scrollable">
					<table id="grid" class="table table-striped table-bordered table-hover dataTable">
					<thead>
					<tr>
									<th>ID</th> 
			<th>MemberNo</th> 
			<th>Account</th> 
			<th>TrueName</th> 
			<th>IDCard</th> 
			<th>WeXinID</th> 
			<th>Mobile</th> 
			<th>ParentID</th> 
			<th>JoinTime</th> 
			<th>AddTime</th> 
			<th>Status</th> 
			<th style="width:120px;">操作</th>

					</tr>
					</thead>
					<tbody>
					
					</tbody>
					</table>							
				</div>
				<div>
					<ul id="Pagination" class="pagination"></ul>
				</div>
			</div>
		</div>
              </div>
            </div>
            <!-- END PAGE CONTENT-->

            <!-- edit -->
            <div class="modal fade" id="mod_info" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true" style="display: none;">
                <div class="modal-dialog">
                    <div class="modal-content">
			<div class="modal-header">							 
				<div class="caption">Member</div>
				<div class ="tools">
				<a href="javascript:;" class="remove hidden-phone"></a>						
                        </div><!-- /.box-header -->
						 
                        <div class="modal-body">
                            	<div class="form-group">
		<label for="txtID">ID</label>
		<input type="text" id="txtID" name="ID" class="form-control" placeholder="请输入ID"/>
	</div>
	<div class="form-group">
		<label for="txtMemberNo">MemberNo</label>
		<input type="text" id="txtMemberNo" name="MemberNo" class="form-control" placeholder="请输入MemberNo"/>
	</div>
	<div class="form-group">
		<label for="txtAccount">Account</label>
		<input type="text" id="txtAccount" name="Account" class="form-control" placeholder="请输入Account"/>
	</div>
	<div class="form-group">
		<label for="txtTrueName">TrueName</label>
		<input type="text" id="txtTrueName" name="TrueName" class="form-control" placeholder="请输入TrueName"/>
	</div>
	<div class="form-group">
		<label for="txtIDCard">IDCard</label>
		<input type="text" id="txtIDCard" name="IDCard" class="form-control" placeholder="请输入IDCard"/>
	</div>
	<div class="form-group">
		<label for="txtWeXinID">WeXinID</label>
		<input type="text" id="txtWeXinID" name="WeXinID" class="form-control" placeholder="请输入WeXinID"/>
	</div>
	<div class="form-group">
		<label for="txtMobile">Mobile</label>
		<input type="text" id="txtMobile" name="Mobile" class="form-control" placeholder="请输入Mobile"/>
	</div>
	<div class="form-group">
		<label for="txtParentID">ParentID</label>
		<input type="text" id="txtParentID" name="ParentID" class="form-control" placeholder="请输入ParentID"/>
	</div>
	<div class="form-group">
		<label for="txtJoinTime">JoinTime</label>
		<input type="text" id="txtJoinTime" name="JoinTime" class="form-control" placeholder="请输入JoinTime"/>
	</div>
	<div class="form-group">
		<label for="txtAddTime">AddTime</label>
		<input type="text" id="txtAddTime" name="AddTime" class="form-control" placeholder="请输入AddTime"/>
	</div>
	<div class="form-group">
		<label for="txtStatus">Status</label>
		<input type="text" id="txtStatus" name="Status" class="form-control" placeholder="请输入Status"/>
	</div>

                        </div>
                        <div class="modal-footer">
                            <a href="#" class="btn btn-default" data-dismiss="modal">取消</a>
                            <a href="#" id="btnOK" class="btn btn-primary">保存</a>
                        </div>
                    </div>
                </div>
            </div><!-- /edit -->
          </div>
        </div>
        <!-- END CONTENT -->
    </div>
    <!-- END CONTAINER -->
    
<%- include shared/_bottom %>
<%- include shared/_scripts %>
	<script src="../assets/global/plugins/bootbox/bootbox.min.js" type="text/javascript"></script>
	<script src="../assets/global/plugins/bootstrap-paginator/bootstrap-paginator.js"></script>	    
	<!-- BEGIN PAGE LEVEL PLUGINS -->
	<script src="../assets/global/plugins/bootstrap-datepicker/bootstrap-datepicker.js" type="text/javascript"></script>
	<script src="../assets/global/plugins/bootstrap-datepicker/locales/bootstrap-datepicker.zh-CN.js" type="text/javascript"></script>    
	<!-- END PAGE LEVEL PLUGINS -->
	<script src="/javascripts/include/date-utils.min.js"></script>
	<script src="/javascripts/include/query.js"></script>
	<script src="/javascripts/Member.js" type="text/javascript"></script>
<%- include shared/_footer %>
