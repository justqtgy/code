    $.get('/vehicle/vhc_group',function(result){       
        $('.multiselect').html(result.group);
        $('.multiselect').multiselect({ nonSelectedText: '请选择',enableFiltering: true, includeSelectAllOption: true, selectAllText: '全部选择', enableClickableOptGroups: true, buttonWidth: 150, maxHeight: 300 });
    })
</script>