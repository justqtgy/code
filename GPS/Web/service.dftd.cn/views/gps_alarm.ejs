
<%- include ./shared/_header %>
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <link href="../assets/global/plugins/bootstrap-datepicker/datepicker3.css" rel="stylesheet" type="text/css" />
    <!-- END PAGE LEVEL PLUGINS -->
<%- include ./shared/_top %>
<%- include ./shared/_sidebar %>        

    <!-- BEGIN CONTENT -->
    <div class="page-content-wrapper">
        <div class="page-content">
            <!-- BEGIN PAGE HEADER-->
            <h3 class="page-title">
                短信告警 <small>漏油告警</small>
            </h3>
                
            <!-- END PAGE HEADER-->
            <div class="box box-solid">
                <div class="box-header with-border">
                    <i class="fa fa-search"></i>
                    <h3 class="box-title">查询</h3>
                </div><!-- /.box-header -->
                <div class="box-body">
                    <form  id='search'>                       
                        <label class="control-label" for="txtBeginTime">开始时间：</label><input type="text" name="begintime"  class="date-picker"  data-date-format="yyyy-mm-dd"/>
                        <label class="control-label" for="txtEndTime">结束时间：</label><input type="text" name="endtime"  class="date-picker"  data-date-format="yyyy-mm-dd"/>
                        <button type="button" id="btnSearch" class="btn btn-success"> <i class="fa fa-search"></i>查询 </button>
                    </form>
                </div><!-- /.box-body -->
            </div>
            <!-- BEGIN PAGE CONTENT-->
            <div class="row">
                <div class="col-md-12">
                    <div class="portlet box green">
                        <div class="portlet-title">
                            <div class="caption">
                                <i class="fa fa-picture"></i> 数据列表
                            </div>                            
                        </div>
                        <div class="portlet-body">
                            <div class="table-scrollable">                  
                                    <table id="grid" class="table table-striped table-bordered table-hover dataTable">
                                        <thead>
                                            <tr>
                                                <th>ID</th> 
                                                <th>车牌号</th> 
                                                <th>GPRS</th> 
                                                <th>电话号码</th>                             
                                                <th>发送时间</th>                             
                                            </tr>
                                        </thead>
                                        <tbody v-if = "DataList.length > 0">
                                            <tr v-for = "item in DataList">
                                                <td>{{ item.ID }}</td> 
                                                <td>{{ item.CarNumber }}</td> 
                                                <td>{{ item.GPRSID }}</td> 
                                                <td>{{ item.Mobile }}</td>                             
                                                <td>{{ new Date(item.AddTime).toUTCFormat('YYYY-MM-DD HH24:MI:SS') }}</td>  
                                            </tr>
                                        </tbody>
                                        <tbody v-else>
                                            <tr><td colspan="5" style="text-align:center;">没有数据</td></tr>
                                        </tbody>
                                    </table>				
                            </div>
                            <div>
                                <ul id="Pagination" class="pagination"></ul>
                            </div>
                        </div>
                    </div>
                </div>	
            </div>
            <!-- END PAGE CONTENT-->
        </div>
    </div>
    <!-- END CONTENT -->

</div>
<!-- END CONTAINER -->
    
<%- include ./shared/_bottom %>
<%- include ./shared/_scripts %>
	<script src="../assets/global/plugins/bootbox/bootbox.min.js" type="text/javascript"></script>
	<script src="../assets/global/plugins/bootstrap-paginator/bootstrap-paginator.js"></script>	    
    <!-- BEGIN PAGE LEVEL PLUGINS -->
    <script src="../assets/global/plugins/moment.min.js" type="text/javascript"></script>
    <script src="../assets/global/plugins/bootstrap-datepicker/bootstrap-datepicker.js" type="text/javascript"></script>
    <script src="../assets/global/plugins/bootstrap-datepicker/locales/bootstrap-datepicker.zh-CN.js" type="text/javascript"></script>    
    <!-- END PAGE LEVEL PLUGINS -->
    <script src="/javascripts/include/query.js"></script>
    <script src="/javascripts/gps_alarm.js"></script>
<%- include ./shared/_footer %>
